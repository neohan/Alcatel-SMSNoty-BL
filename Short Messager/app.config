                </binding>
            </customBinding>
        </bindings>
        <client>
            <endpoint address="http://121.199.48.186:1210/Services/MsgSend.asmx" binding="basicHttpBinding" bindingConfiguration="MsgSendSoap" contract="TopenServiceReference.MsgSendSoap" name="MsgSendSoap"/>
        </client>
